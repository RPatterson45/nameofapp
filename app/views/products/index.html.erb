<h1 id= "products-title">Products</h1>
<% cache(cache_key_for_products) do %>
  <table>
    <tbody>
      <div class="container">
          <div class="row">
            <% @products.each do |product| %>
              <%= render partial: 'product', locals: { product: product } %>
            <% end %>
          </div>
        </div>
    </tbody>
  </table>
<% end %>

<br>

<%= link_to image_tag("http://www.apdigitalbee.com/zhfast/image/catalog/icon/new-product-sticker.png", class: "img-responsive new-product"), new_product_path %>
